<% if activity.trackable %>
<div class="user-box">
<%#= image_tag activity.owner.picture.url(:thumb), class: 'like_profile' %>
<%= link_to activity.owner.fname, activity.owner %>
liked a <%= link_to activity.trackable.likeable.class, user_status_path(activity.trackable.likeable) %>
</div>
<div class="like_post">
<%= render 'statuses/status', status: activity.trackable.likeable %>
</div>
<% end %>