<% if activity.trackable %>
<div class="activity_feed">
<img src="http://placehold.it/80x80&text=[img]"/>
<%= link_to activity.owner.fname, activity.owner %> added a post <%= link_to activity.trackable.post, user_status_path(activity.trackable) %>
<%= render 'statuses/status', status: activity.trackable %>
<% end %>
</div>