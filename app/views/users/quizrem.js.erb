
var old = parseInt($('#score').val());
$('#score').val(old+<%= @marks %>);

$('#quizload').hide();
<% if @question.nil? %>
	$('#questions').html('Thanks');
	alert('Your Score..' + $('#score').val());
<% else %>
	$('#questions').html("<%= escape_javascript(render(:partial => 'question', :locals => {:question => @question})) %>");
<% end %>

